<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd"
    objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
  <changeSet id="1" author="jb2">
    <createTable tableName="JB2_EMPLOYER">
      <column name="id" type="VARCHAR(255)">
        <constraints nullable="false" primaryKey="true" primaryKeyName="PK_JB2_EMPLOYER"/>
      </column>
      <column name="NAME" type="VARCHAR(255)"/>
    </createTable>
  </changeSet>
  <changeSet id="2" author="jb2">
    <createTable tableName="JB2_VACANCY">
      <column name="id" type="VARCHAR(255)">
        <constraints nullable="false" primaryKey="true" primaryKeyName="PK_JB2_VACANCY"/>
      </column>
      <column name="name" type="VARCHAR(255)"/>
    </createTable>
  </changeSet>
</databaseChangeLog>